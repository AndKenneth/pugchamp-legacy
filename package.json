{
  "name": "pugchamp",
  "description": "PUG service",
  "private": true,
  "repository": "fwdcp/pugchamp",
  "dependencies": {
    "body-parser": "^1.14.2",
    "botkit": "0.0.5",
    "chance": "^0.8.0",
    "co": "^4.6.0",
    "config": "^1.17.1",
    "connect-mongo": "^1.1.0",
    "distributions": "^0.2.1",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "hbs": "^4.0.0",
    "heapdump": "^0.3.7",
    "js-combinatorics": "^0.5.0",
    "jsonwebtoken": "^5.5.4",
    "lodash": "^4.0.0",
    "mathjs": "^2.6.0",
    "moment": "^2.11.1",
    "moment-duration-format": "^1.3.0",
    "mongoose": "^4.3.1",
    "ms": "^0.7.1",
    "mz": "^2.1.0",
    "passport": "^0.3.2",
    "passport-openid": "fwdcp/passport-openid",
    "q": "^1.4.1",
    "request-promise": "^2.0.0",
    "serve-static": "^1.10.0",
    "socket.io": "^1.3.7",
    "socketio-jwt": "^4.3.3",
    "srcds-rcon": "fwdcp/node-srcds-rcon"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "array-callback-return": 2,
      "arrow-body-style": 2,
      "arrow-parens": [2, "as-needed"],
      "arrow-spacing": 2,
      "block-scoped-var": 2,
      "brace-style": [2, "stroustrup"],
      "callback-return": 2,
      "comma-style": 2,
      "curly": 2,
      "dot-location": 2,
      "dot-notation": 2,
      "eqeqeq": 2,
      "generator-star-spacing": [2, "after"],
      "global-require": 2,
      "handle-callback-err": 2,
      "key-spacing": 2,
      "no-confusing-arrow": 2,
      "no-magic-numbers": 2,
      "no-mixed-requires": 2,
      "no-new-require": 2,
      "no-path-concat": 2,
      "no-process-exit": 2,
      "no-shadow": 2,
      "no-throw-literal": 2,
      "no-unneeded-ternary": 2,
      "no-use-before-define": 2,
      "object-shorthand": 2,
      "operator-assignment": 2,
      "prefer-spread": 2,
      "prefer-template": 2,
      "require-yield": 2,
      "quotes": [2, "single"],
      "template-curly-spacing": 2,
      "yoda": 2
    }
  }
}
